[user]
        name = Omar Sabry
        email = ohsabry@gmail.com
[hub]
        protocol = https
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
[alias]
	unstage = reset
	uncommit = reset --soft HEAD^
	mup = checkout master && git pull && git checkout -
[push]
	default = upstream
[fetch]
	prune = true
[merge]
	ff = only
